<h1>Houses</h1>
<h3><%= link_to "Add a House", new_house_path %></h3>
<% @houses.each do |house| %>
  <div>
    <h2><%= link_to "#{house.name}", house_path(house) %></h2>
    <h3 class='quote'><%="#{house.quote}"%></h3>
    <p><img class = "crest" src="<%= house.img_url %>"/></p>
  </div>
<% end %>
